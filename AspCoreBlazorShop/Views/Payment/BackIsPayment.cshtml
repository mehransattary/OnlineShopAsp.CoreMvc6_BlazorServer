@{
    ViewData["Title"] = " نتیجه برگشت از درگاه";


    var Message = ViewBag.Message;
    var RefId = ViewBag.RefId;
}
<!-- Hero Start -->
<section class="bg-half bg-light d-table w-100">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-12 text-center">
                <div class="page-next-level">
                    <h4 class="title"> برگشت از درگاه </h4>
                    <div class="page-next">
                        <nav aria-label="breadcrumb" class="d-inline-block">
                            <ul class="breadcrumb bg-white rounded shadow mb-0">
                                <li class="breadcrumb-item"><a href="/">خانه </a></li>
                                <li class="breadcrumb-item active" aria-current="page">برگشت از درگاه </li>
                            </ul>
                        </nav>
                    </div>
                </div>
            </div><!--end col-->
        </div><!--end row-->
    </div> <!--end container-->
</section><!--end section-->
<!-- Start -->
<section class="section">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <div class="table-responsive bg-white shadow">
                    <table class="table table-center table-padding mb-0">
                        <thead>
                            <tr>
                                <th class="border-bottom py-3 text-center" >وضعیت سفارش</th>
                                <th class="border-bottom py-3  text-center">شناسه پرداخت </th>

                            </tr>
                        </thead>

                        <tbody>
                            <tr class="shop-list">
                                <td class="text-center">
                                    @ViewBag.Message
                                </td>
                                <td class="text-center">
                                    @ViewBag.RefId
                                </td>
                            </tr>

                        </tbody>
                    </table>
                </div>
            </div><!--end col-->
        </div><!--end row-->

    </div><!--end container-->
</section><!--end section-->
<!-- End -->
